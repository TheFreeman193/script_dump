"UnlitGeneric"
{
	"$basetexture" "phoenix_storms/t_light_us"
	"$selfillum" 1
	"$additive" 0.2
	"$envmap" "env_cubemap"
	"$envmapcontrast" 1.2
	"$envmapsaturation" 0.5
	"$envmaptint" "[ .8 .8 .8 ]"
	"$model" 1
	"$halflambert" 1

	"Proxies"
	{
		"AnimatedTexture" 
         	{ 
     		    "animatedTextureVar" "$basetexture" 
      	 	    "animatedTextureFrameNumVar" "$frame" 
       	 	    "animatedTextureFrameRate" "0.4" 
        	 }

		
	}
}


